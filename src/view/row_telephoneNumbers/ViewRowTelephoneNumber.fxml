<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <TextField fx:id="txtfld_telephoneNumber" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
      </TextField>
   </center>
   <left>
      <ComboBox fx:id="cmbx_telephoneType" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
      </ComboBox>
   </left>
   <right>
      <Button fx:id="btn_delete" minWidth="30.0" mnemonicParsing="false" onAction="#OnAction_delete" prefWidth="30.0" text="-" textFill="RED" BorderPane.alignment="CENTER">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
         <BorderPane.margin>
            <Insets bottom="5.0" left="30.0" right="5.0" top="5.0" />
         </BorderPane.margin>
      </Button>
   </right>
</BorderPane>
